{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "VM Creation Job",
  "properties": {
      "email_id": {
          "type": "string",
          "default": "",
          "title": "Enter The Email-id-:",
          "examples": [
              "test@devtron.ai"
          ]
      },
      "no_of_cores": {
          "type": "string",
          "default": "2",
          "title": "Enter the no_of_cores do you want-: 2/4/8",
          "examples": [
              "2"
          ]
      },
      "volume_size": {
          "type": "string",
          "default": "30",
          "title": "Enter the volume size you want-: ",
          "examples": [
              "30"
          ]
      },
      "want_to_install_microk8s": {
          "type": "boolean",
          "default": true,
          "title": "Do you want to install microk8s ",
          "examples": [
              true
          ]
      },
      "want_to_install_devtron": {
          "type": "boolean",
          "default": true,
          "title": "Do you want to install Devtron",
          "examples": [
              true
          ]
      },
      "NOTE": {
        "type": "string",
        "default": "ALL THE CREADENTIALS WILL BE SHARE TO YOU IN THE EMAIL",
        "title": "NOTE-:",
        "examples": [
            true
        ]
    }
  },
  "NOTE": "ALL THE CREDENTIALS WILL BE SHARED TO YOU IN THE EMAIL."

}